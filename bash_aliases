alias be='bundle exec'
alias gc='git commit'
alias gco='git checkout'
alias gd='git diff'
alias gg='git grep'
alias gl='git log'
alias ll='ls -l'
alias gp='git pull --rebase && git submodule update --init --recursive'
alias gpo='git push origin'
alias gs='git status'
alias ssha='ssh-agent | head -2 > ~/.ssh-agent'
alias sa='ssh-add -t $((8*3600))'
alias spek='spec -f nested -c -b '
alias trt='touch tmp/restart.txt'
alias bilp='bundle install --local --path vendor/bundle'
