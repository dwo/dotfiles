alias be='bundle exec'
alias dc='docker-compose'
alias dr='docker run'
alias gc='git commit'
alias gco='git checkout'
alias gd='git diff'
alias gdw='git diff --word-diff'
alias gg='git grep'
alias gl='git log'
alias glp='git log -p'
alias gp='git pull --rebase && git submodule update --init --recursive'
alias gpo='git push origin'
alias gs='git status'
alias java11='export JAVA_HOME=(/usr/libexec/java_home -v11)'
